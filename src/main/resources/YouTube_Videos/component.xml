<component name="YouTube_Videos" type="synapse/template">
    <subComponents>
        <component name="VideosList">
            <file>VideosList.xml</file>
            <description>Returns a list of videos that match the API request parameters.</description>
        </component>
        <component name="VideosInsert">
            <file>VideosInsert.xml</file>
            <description>Uploads a video to YouTube and optionally sets the video's metadata.</description>
        </component>
        <component name="VideosUpdate">
            <file>VideosUpdate.xml</file>
            <description>Updates a video's metadata.</description>
        </component>
        <component name="VideosRate">
            <file>VideosRate.xml</file>
            <description>Add a like or dislike rating to a video or remove a rating from a video.</description>
        </component>
        <component name="VideosGetRating">
            <file>VideosGetRating.xml</file>
            <description>Retrieves the ratings that the authorized user gave to a list of specified videos.
            </description>
        </component>
        <component name="VideosDelete">
            <file>VideosDelete.xml</file>
            <description>Deletes a YouTube video.</description>
        </component>
    </subComponents>
</component>
